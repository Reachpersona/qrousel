{
  "name": "qrousel",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://reachpersona.github.io/qrousel/",
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "js-yaml": "^4.1.0",
    "marked": "^15.0.7",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  },
  "scripts": {
    "yaml-to-json": "node yaml-to-json.js",
    "start": "npm run yaml-to-json && react-scripts start",
    "build": "react-scripts build",
    "build-test": "REACT_APP_GH_PAGES=$1 node yaml-to-json.js ./data/qrdata-test.yaml",
    "test": "npm run build-test && react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build-gh-pages https://reachpersona.github.io/qrousel/",
    "deploy": "gh-pages -d build ",
    "build-gh-pages": "REACT_APP_GH_PAGES=$1 node yaml-to-json.js && react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
